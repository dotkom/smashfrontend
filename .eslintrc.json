{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb", "react-app"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-underscore-dangle": "off",
      "react/destructuring-assignment": "off",
      "react/forbid-prop-types": ["off"]
    }
}
